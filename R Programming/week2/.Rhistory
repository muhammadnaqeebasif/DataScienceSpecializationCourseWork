corr <-function(directory,threshold=0){
lst = list.dirs(directory)
cor_vector<- vector()
for(id in seq_along(lst)){
completeness <-complete(directory = directory,id=id)
if(completeness[['nobs']]<=threshold){
cor_vector <-c(cor_vector,0)
}
else{
df <- read.csv(lst[id])
cor_vector <- c(cor_vector,cor(df[['sulfate']],df[['nitrate']]))
}
}
cor_vector
}
